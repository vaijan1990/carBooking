html
  head
    
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    meta(name='description' content='')
    meta(name='author' content='Grupp 9')
    
    title #{title} - Car Rental Supreme
    
    link(rel='stylesheet', href='public/vendor/bootstrap/css/bootstrap.min.css', type='text/css')
    //link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/css/tether.css', type='text/css')
    link(rel='stylesheet', href='public/vendor/theme-freelancer/freelancer.min.css', type='text/css')
    link(rel='stylesheet', href='public/vendor/font-awesome/css/font-awesome.min.css', type='text/css')
    link(rel='stylesheet', href='public/main.min.css', type='text/css')
    block extra-css
    link(href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic' rel='stylesheet' type='text/css')

  body(id='page-top' class='index')
    
    nav(id='mainNav' class='navbar fixed-top navbar-toggleable-md navbar-light')
      button(type='button' class='navbar-toggler navbar-toggler-right' data-toggle='collapse' data-target='#main-navbar-collapse' aria-controls='navbarExample' aria-expanded='false' aria-label='Toggle navigation') Menu
        i(class='fa fa-bars')
        
      div(class='container')
        a(class='navbar-brand' href='#page-top') Car Rental
          |
          |
          em Supreme
        div(class='collapse navbar-collapse' id='main-navbar-collapse')
          ul(class='navbar-nav ml-auto')
            li(class='nav-item')
              a(href='#ourcars' class='nav-link') Our cars
            li(class='nav-item')
              a(href='#aboutus' class='nav-link') About us
            li(class='nav-item')
              a(id="loginId" class='nav-link' data-toggle="modal" data-target="#exampleModal") Login

    include includes/loginform
    block header
    block main-content
  
      
    footer(class='text-center')
      div(class='footer-above')
        div(class='container')
          div(class='row')
            div(class='footer-col col-md-4')
              h3 Location
              p 3481 Melrose Place
              br
              .
                Beverly Hills, CA 90210

            div(class='footer-col col-md-4')
              h3 Around the Web
              ul(class='list-inline')
                li(class='list-inline-item')
                  a(class='btn-social btn-outline' href='#')
                    i(class='fa fa-fw fa-facebook')
                li(class='list-inline-item')
                  a(class='btn-social btn-outline' href='#')
                    i(class='fa fa-fw fa-google-plus')
                li(class='list-inline-item')
                  a(class='btn-social btn-outline' href='#')
                    i(class='fa fa-fw fa-twitter')
                li(class='list-inline-item')
                  a(class='btn-social btn-outline' href='#')
                    i(class='fa fa-fw fa-linkedin')
                li(class='list-inline-item')
                  a(class='btn-social btn-outline' href='#')
                    i(class='fa fa-fw fa-dribbble')

            div(class='footer-col col-md-4')
              h3 About Freelancer
              p Freelance is a free to use, open source Bootstrap theme created by 
                a(href='http://startbootstrap.com')
                .
                  Start Bootstrap

      div(class='footer-below')
        div(class='container')
            div(class='row')
                div(class='col-lg-12')
                    Copyright &copy; Your Website 2017

    
    //Scroll to Top Button (Only visible on small and extra-small screen sizes)
    div(class='scroll-top hidden-lg-up')
      a(class='btn btn-primary page-scroll' href='#page-top')
        i(class='fa fa-chevron-up')

    script(src='public/vendor/jquery/jquery.min.js')
    //script(src='public/dependencies/theme-freelancer/js/tether.js')
    script(src='public/vendor/bootstrap/js/bootstrap.min.js')
    script(src='public/vendor/jquery-easing/jquery.easing.js')
    script(src='public/vendor/theme-freelancer/freelancer.min.js')
    script(src='public/js/bootstrapModal.js')
    script(src='public/js/main.js')
    block extra-js
